{
  "family": "bitnagil-dev",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "bitnagil-dev",
      "cpu": 1,
      "portMappings": [
        {
          "containerPort": 8081,
          "hostPort": 0,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "dev"
        },
        {
          "name": "TZ",
          "value": "Asia/Seoul"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/bitnagil-dev",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::750819668269:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::750819668269:role/ecsTaskExecutionRole",
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "1024",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}