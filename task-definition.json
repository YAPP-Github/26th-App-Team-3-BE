{
  "family": "bitnagil-prod",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "bitnagil",
      "image": "750819668269.dkr.ecr.ap-northeast-2.amazonaws.com/bitnagil:latest",
      "cpu": 1.5,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/spring-app",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "spring"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::750819668269:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::750819668269:role/ecsTaskExecutionRole",
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
